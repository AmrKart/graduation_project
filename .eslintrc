{
  "rules": {
    "no-undef": "off",
    "@typescript-eslint/no-explicit-any": ["off"]

  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
